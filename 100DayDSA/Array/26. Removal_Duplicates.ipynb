   "source": [
    "# Sort Inplace \n",
    "\n",
    "def removal_duplicates(nums):\n",
    "    nums[:]=sorted(set(nums)) # nums[:] -> what this does is it replace elements inplace \n",
    "    return len(nums)\n",
    "\n",
    "nums=[1,1,2]\n",
    "removal_duplicates(nums)\n"
   ]
